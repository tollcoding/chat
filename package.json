{
  "name": "prochat",
  "version": "1.0.2",
  "private": true,
  "scripts": {
    "start": "NODE_PATH=.;NODE_ENV=production node ./bin/www",
    "coveragenyc": "nyc --check-coverage --lines 100  --timeout 15000 npm run coverage",
    "lint": "eslint *.js test/",
    "test": "NODE_PATH=.;NODE_ENV=test mocha --compilers js:babel-register",
    "test:win": "mocha test",
    "coverage": "rm -rf coverage/* && babel-node ./node_modules/.bin/istanbul cover _mocha -- --compilers js:babel-register",
    "dev": "NODE_PATH=.;NODE_ENV=development nodemon ./bin/www --exec node",
    "compile": "rm -rf dist/* && babel ./src -d dist"
  },
  "dependencies": {
    "async": "^2.5.0",
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.11.6",
    "bluebird": "^3.5.0",
    "body-parser": "~> 1.17.2",
    "connect": "^3.6.2",
    "connect-mongo": "~> 1.3.2",
    "cookie": "~> 0.3.1",
    "cookie-parser": "~> 1.4.3",
    "cool-ascii-faces": "~> 1.3.4",
    "crypto": "~1.0.1",
    "debug": "^3.0.0",
    "ejs": "~> 2.5.5",
    "ejs-locals": "~> 1.0.2",
    "errorhandler": "~> 1.5.0",
    "express": "^4.15.4",
    "express-jquery": "~> 1.8.3",
    "express-session": "^1.15.3",
    "handlebars": "^4.0.10",
    "mongodb": "^2.2.31",
    "mongoose": "^4.11.7",
    "morgan": "~> 1.8.2",
    "nconf": "~> 0.8.4",
    "serve-favicon": "~> 2.4.3",
    "socket.io": "~> 2.0.3",
    "winston": "~> 2.3.1"
  },
  "devDependencies": {
    "casperjs": "^1.1.4",
    "chai": "^4.1.1",
    "eslint": "^4.4.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.2.1",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.4.1",
    "nodemon": "^1.10.0",
    "nyc": "^11.1.0",
    "standard": "^10.0.3",
    "supertest": "^3.0.0",
    "toboggan": "^1.0.0"
  }
}
