{
  "name": "prochat",
  "version": "1.0.2",
  "private": true,
  "scripts": {
    "start": "NODE_PATH=.;NODE_ENV=production node ./bin/www",
    "coveragenyc": "nyc --check-coverage --lines 100  --timeout 15000 npm run coverage",
    "lint": "eslint *.js test/",
    "test": "NODE_PATH=.;NODE_ENV=test mocha --compilers js:babel-register",
    "test:win": "mocha test",
    "coverage": "rm -rf coverage/* && babel-node ./node_modules/.bin/istanbul cover _mocha -- --compilers js:babel-register",
    "dev": "NODE_PATH=.;NODE_ENV=development nodemon ./bin/www --exec node",
    "compile": "rm -rf dist/* && babel ./src -d dist"
  },
  "dependencies": {
    "async": "^2.5.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.26.0",
    "bluebird": "^3.5.1",
    "body-parser": "~1.18.1",
    "connect": "^3.6.5",
    "connect-mongo": "~2.0.0",
    "cookie": "~> 0.3.1",
    "cookie-parser": "~> 1.4.3",
    "cool-ascii-faces": "~> 1.3.4",
    "crypto": "^1.0.1",
    "debug": "^4.0.0",
    "ejs": "~> 2.5.5",
    "ejs-locals": "~> 1.0.2",
    "errorhandler": "~> 1.5.0",
    "express": "^4.16.2",
    "express-jquery": "~> 1.8.3",
    "express-session": "^1.15.6",
    "handlebars": "^4.0.11",
    "mongodb": "^3.0.1",
    "mongoose": "^5.0.1",
    "morgan": "~1.9.0",
    "nconf": "~0.10.0",
    "serve-favicon": "^2.4.5",
    "socket.io": "^2.0.4",
    "winston": "~3.0.0-rc0"
  },
  "devDependencies": {
    "casperjs": "^1.1.4",
    "chai": "^4.1.2",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^5.0.0",
    "nodemon": "1.18.5",
    "nyc": "^12.0.1",
    "prettier": "1.14.3",
    "standard": "^11.0.0",
    "supertest": "^3.0.0",
    "toboggan": "^1.0.0"
  }
}
